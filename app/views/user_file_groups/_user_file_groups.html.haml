%table.table.table-striped
  %thead
    %tr
      %th Name
      %th

  %tbody
    - @user_file_groups.each do |user_file_group|
      %tr
        %td= raw file_group_labels(user_file_group)
        %td
          = link_to 'Bearbeiten', edit_user_file_group_path(user_file_group),
          class: 'btn btn-default'
          = link_to 'Entfernen', user_file_group, :method => :delete, 
          class: 'btn btn-danger',
          :data => { :confirm => 'Are you sure?' }
